{
  "commands": "./lib/commands",
  "bin": "smartui-migrate",
  "dirname": "lib",
  "hooks": {},
  "plugins": [],
  "pjson": {
    "name": "smartui-migration-tool",
    "version": "1.0.0",
    "description": "A sophisticated CLI tool to migrate visual testing suites to LambdaTest SmartUI",
    "main": "lib/index.js",
    "bin": {
      "smartui-migrate": "./bin/run"
    },
    "scripts": {
      "build": "tsc",
      "dev": "tsc --watch",
      "start": "node lib/index.js",
      "test": "echo \"Error: no test specified\" && exit 1",
      "lint": "eslint src --ext .ts",
      "lint:fix": "eslint src --ext .ts --fix",
      "format": "prettier --write src/**/*.ts"
    },
    "keywords": [
      "smartui",
      "migration",
      "visual-testing",
      "percy",
      "applitools",
      "sauce-labs",
      "lambdatest",
      "cli"
    ],
    "author": "LambdaTest",
    "license": "MIT",
    "dependencies": {
      "@oclif/core": "^3.0.0",
      "chalk": "^5.3.0",
      "figlet": "^1.7.0"
    },
    "devDependencies": {
      "@types/node": "^20.0.0",
      "@typescript-eslint/eslint-plugin": "^6.0.0",
      "@typescript-eslint/parser": "^6.0.0",
      "eslint": "^8.0.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.0",
      "prettier": "^3.0.0",
      "typescript": "^5.0.0"
    },
    "engines": {
      "node": ">=18.0.0"
    },
    "files": [
      "lib",
      "bin"
    ],
    "oclif": {
      "commands": "./lib/commands",
      "bin": "smartui-migrate",
      "dirname": "lib",
      "hooks": {},
      "plugins": []
    }
  },
  "version": "1.0.0"
}
